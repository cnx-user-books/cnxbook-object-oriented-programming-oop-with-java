public class Q13{
  ArrayList &lt;Integer&gt; var1 = new ArrayList&lt;Integer&gt;();
  
  void runIt(){
  
    var1.add(new Integer(1));
    var1.add(new Integer(2));
    var1.add(new Integer(3));
    
    //Get an iterator
    Iterator iter = var1.iterator();
    
    //Perform the iteration
    while(iter.hasNext()){
      System.out.println(iter.next().intValue());
    }//end while loop
    
  }//end runIt
  
  public static void main(String[] args){
