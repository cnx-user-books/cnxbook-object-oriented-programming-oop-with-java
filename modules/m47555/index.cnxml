<row id="tr1025">

				

<entry id="td1012">



<code id="pre1012" display="block">
/*File A13.java 
************************************************/

import java.util.*;

public class A13{
  ArrayList &lt;Integer&gt; var1 = new ArrayList&lt;Integer&gt;();
	
  void runIt(){
  
    var1.add(new Integer(1));
    var1.add(new Integer(2));
    var1.add(new Integer(3));
    
    //Get an iterator
    Iterator &lt;Integer&gt; iter = var1.iterator();
    
    //Perform the iteration
    while(iter.hasNext()){
      System.out.println(iter.next().intValue());
    }//end while loop
    
  }//end runIt
  
  public static void main(String[] args){
    new Q13().runIt();
  }//end main

}//end class A13
//=============================================//
</code>



				

